    <body>

        <%- include('Header.ejs') %>

		<main>
			<div id="printJson">

			</div>

			<div>
				<div class="centerbutton">
					<div class="centerbuttoninput">
						<button id="selectReleaseDate"> Select by release date  </button>
						<input id="releaseDataInput"> </input>
						<button id="usedValues"> Used values </button>
					</div>
